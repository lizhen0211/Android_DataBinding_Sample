<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.databindingsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".simplebinding.SimpleBindingActivity" />
        <activity android:name=".bindingevents.BindingEventActivity" />
        <activity android:name=".aliasandnullcoalescing.AliasAndNullCoalescing" />
        <activity android:name=".custombinding.CustomBinding" />
        <activity android:name=".includes.IncludesActivity" />
        <activity android:name=".observableobjects.ObservableActivity" />
        <activity android:name=".viewswithids.ViewsWithIDs" />
        <activity android:name=".dynamicvariables.DynamicVariables" />
        <activity android:name=".attributesetters.AttributeSetters"></activity>
    </application>

</manifest>